<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Empire City Elixir Conference" %></title>

    <%= stylesheet_link_tag "all" %>
    <%= stylesheet_link_tag "//cdn.jsdelivr.net/jquery.slick/1.5.9/slick.css" %>
    <%= stylesheet_link_tag "//cdn.jsdelivr.net/jquery.slick/1.5.9/slick-theme.css" %>

    <%= partial "partials/meta" %>
    <%= favicon_tag "favicon.ico" %>

    <%= yield_content :head %>
  </head>

  <body class="<%= page_classes %>">
    <%= yield_content :hero %>
    <div class="navbar-spacer"></div>
    <%= partial "partials/navbar" %>
    <main>
      <%= yield %>
    </main>
    <%= partial "partials/bottom" %>
    <%= partial "partials/analytics" %>
    <%= javascript_include_tag "vendor/jquery-2.2.1.min.js" %>
    <%= javascript_include_tag "//cdn.jsdelivr.net/jquery.slick/1.5.9/slick.min.js" %>
    <%= javascript_include_tag "nav"%>
    <%= yield_content :bottom_of_page %>
  </body>
</html>
